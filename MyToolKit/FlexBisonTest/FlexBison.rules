<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Flex and Bison Tools"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="Bison"
			DisplayName="Bison"
			CommandLine=" C:\win_flex_bison\win_bison.exe -d -v -b y $(InputPath)"
			Outputs="y.tab.c,y.tab.h"
			FileExtensions="*.y"
			ExecutionDescription="Running Bison"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="Flex"
			DisplayName="Flex"
			CommandLine="C:\win_flex_bison\win_flex.exe --wincompat $(InputPath)"
			Outputs="lex.yy.c"
			FileExtensions="*.l;*.flex"
			ExecutionDescription="Running Flex"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
